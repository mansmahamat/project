{
  "expo": {
    "name": "Vibe Boxing",
    "slug": "vibeboxing",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/vibeboxing.png",
    "scheme": "vibeboxing",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "description": "VibeBoxing is a boxing training app with gamification, progress tracking, and professional workouts",
    "keywords": [
      "boxing",
      "fitness",
      "workout",
      "training",
      "martial arts",
      "vibe",
      "vibeboxing"
    ],
          "ios": {
        "supportsTablet": true,
        "bundleIdentifier": "com.vibeboxing.vibeboxing",
        "infoPlist": {
          "ITSAppUsesNonExemptEncryption": false,
          "NSCameraUsageDescription": "VibeBoxing uses the camera to play instructional workout videos and enhance your boxing training experience.",
          "NSMicrophoneUsageDescription": "VibeBoxing uses the microphone to provide audio feedback and coaching during your boxing workouts."
        }
      },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/vibeboxing.png",
        "backgroundColor": "#FF6B35"
      },
      "package": "com.vibeboxing.vibeboxing"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
        "plugins": [
      "expo-router", 
      "expo-font", 
      "expo-web-browser",
      [
        "expo-build-properties",
        {
          "android": {
            "newArchEnabled": true
          },
          "ios": {
            "newArchEnabled": true
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "6f905ab7-8b8e-403b-a010-7720d2d728b9"
      }
    }
  }
}
